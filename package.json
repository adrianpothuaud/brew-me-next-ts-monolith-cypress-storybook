{
  "name": "brew-me-next-ts-monolith",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "at": "cypress open --e2e --browser chrome",
    "at:ci": "cypress run --e2e --browser chrome --record --key 6f9537cd-1419-422a-9a3a-7543c57dbe0d",
    "atdd": "start-server-and-test dev http://localhost:3000 at",
    "atdd:ci": "start-server-and-test start http://localhost:3000 at",
    "build": "next build",
    "ct": "cypress open --component --browser chrome",
    "ct:ci": "cypress run --component --browser chrome --record --key 6f9537cd-1419-422a-9a3a-7543c57dbe0d",
    "dev": "next dev",
    "lint": "next lint",
    "prepare": "husky install",
    "prisma:setup": "prisma db push && prisma generate",
    "start": "next start",
    "stylelint": "stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "classnames": "^2.3.1",
    "joi": "^17.6.0",
    "next": "12.2.5",
    "nodemailer": "^6.7.8",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "cypress": "^10.6.0",
    "eslint": "^8.2.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.1",
    "prisma": "^4.2.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.10.0",
    "stylelint-config-standard": "^27.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
